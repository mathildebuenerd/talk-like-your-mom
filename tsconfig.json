{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "removeComments": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    ".git",
    "dist",
    "node_modules"
  ]
}